    <obstacle x1="12.5" y1="-1.0" x2="12.5" y2="-4.0" type="line"/>


    <!-- Waypoints required for social force -->
    <waypoint id="p1" x="9" y="-1" r="2"/>
    <waypoint id="p2" x="6" y="4" r="2"/>
    <!-- Add pedestrians -->
    <agent x="6" y="4" n="1" dx="0" dy="0" type="1">
        <addwaypoint id="p1"/>
        <addwaypoint id="p2"/>
    </agent>
    <!-- shan -->
