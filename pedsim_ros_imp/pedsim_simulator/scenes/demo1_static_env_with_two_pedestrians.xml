    <obstacle x="-10.5" y="-2.0" xHalfLength="0.1" yHalfLength="0.6" type="rectangle"/>


    <!-- Waypoints required for social force -->
    <waypoint id="p3" x="-5" y="-5" r="2"/>
    <waypoint id="p4" x="-4" y="5" r="2"/>

    <waypoint id="robot_goal" x="15" y="0" r="0.5"/>
    <!-- Add robot (type=2): all the parameters are invalid. The robot pose is aligned with the parameters in pedsim_simulator.launch -->
    <agent x="-5" y="0" n="1" dx="0" dy="0" type="2">
        <addwaypoint id="robot_goal"/>
    </agent>
    <!-- Add pedestrians -->
    <agent x="-4" y="5" n="1" dx="0" dy="0" type="1">
        <addwaypoint id="p3"/>
        <addwaypoint id="p4"/>
    </agent>
</scenario>


